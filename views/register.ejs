<h1>Registration</h1>

<form action="/register" method="post">
  <input type="text" name="email" placeholder="Email...">
  <input type="password" name="password" placeholder="Password...">
  <input type="submit" value="Register">
</form>

<% if (messages.error) { %>
  <p><%- messages.error -%></p>
<% } %>